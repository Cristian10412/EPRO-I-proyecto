{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "17.0.1"
      }
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "unicorn",
      "prettier"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "pascalCase": true
          },
          "ignore": [
            "^index\\.js$",
            "^index\\.jsx$",
            "^electron\\.js$",
            "^www\\.js$"
          ]
        }
      ],
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3100",
  "devDependencies": {
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "prettier": "2.2.1"
  }
}
